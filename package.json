{
	"name": "@paul80nd/monaco-languages",
	"version": "1.10.0",
	"description": "Bundle of Relay Computer Assembly langauges for the Monaco Editor.",
	"scripts": {
		"compile": "mrmdir ./release && tsc -p ./src/tsconfig.json && tsc -p ./src/tsconfig.esm.json",
		"watch": "tsc -p ./src --watch",
		"watch-esm": "tsc -p ./src/tsconfig.esm.json --watch",
		"test": "tape -r ./test/all.js",
		"prepublishOnly": "npm run compile && node ./scripts/bundle"
	},
	"author": "Paul Law",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/paul80nd/monaco-languages"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	},
	"devDependencies": {
		"@types/tape": "^4.2.34",
		"glob": "^7.1.6",
		"jsdom": "^16.2.1",
		"monaco-editor-core": "0.20.0",
		"monaco-plugin-helpers": "^1.0.2",
		"requirejs": "^2.3.6",
		"tape": "^4.13.2",
		"terser": "^4.6.6",
		"typescript": "3.7.5"
	}
}
